{
  "name": "commons-ui",
  "description": "Common static components.",
  "author": "Code for Africa <hello@codeforafrica.org> (https://codeforafrica.org)",
  "license": "GPL-3.0-or-later",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "5.3.18",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-docs": "5.3.18",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-storyshots": "5.3.18",
    "@storybook/addon-storysource": "5.3.18",
    "@storybook/addon-viewport": "5.3.18",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@storybook/source-loader": "*",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.3",
    "jest": "*",
    "lerna": "^3.20.2",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-is": "^16.8.0",
    "regenerator-runtime": "*",
    "typescript": "^3.5.2",
    "webpack": ">=4"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "yarn eslint --fix --ignore-path './.gitignore' --ext '.js,.json' './'",
    "lint-staged": "yarn eslint --fix --ignore-path './.gitignore' --ext '.js,.json'",
    "start": "start-storybook",
    "build:lib": "yarn copy-svg-to-lib && yarn copy-png-to-lib",
    "build:storybook": "build-storybook -o docs/",
    "build": "lerna exec --parallel -- babel --root-mode upward src -d build --copy-files --ignore **/*.story.js,**/*.spec.js",
    "copy-svg-to-lib": "cpx \"./src/**/*.svg\" ./dist",
    "copy-png-to-lib": "cpx \"./src/**/*.png\" ./dist"
  },
  "dependencies": {
    "@material-ui/core": "^4.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,md}": [
      "yarn lint-staged",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.16.0 <=13.x"
  },
  "workspaces": [
    "packages/*"
  ]
}
